<div class="row">
  <div class="col-md-12">
    <div class="card card-default alert alert-success text-center">
    <%= form_for :session, url: login_path do |f| %>
      <%= f.label :email_address %><br>
      <%= f.text_field :email_address %><br>
      <%= f.label :password %><br>
      <%= f.password_field :password %><br><br>
      <%= f.submit "Log In", class: "btn btn-success" %>
    <% end %>
    </div>
  </div>
</div>
