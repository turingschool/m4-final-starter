<h1>Login</h1>

<%= form_for :session, url: sessions_path do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %><br/>

  <%= f.label :password %>
  <%= f.password_field :password %><br/>

  <%= f.submit "Log In"%>
<% end %>

<p>Don't have an account? <%= link_to "Sign Up", new_user_path %></p>