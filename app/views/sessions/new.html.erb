<h1>Login or Sign Up</h1>
<%= form_tag(login_path) do |f| %>
  <%= label_tag :email_address %>
  <%= text_field_tag :email_address, params[:email] %>

  <%= label_tag :password %>
  <%= password_field_tag :password %>

  <%= submit_tag 'Login' %>
<% end %>
<%= link_to 'Create Account', new_user_path %>
