<!DOCTYPE html>
<html>
<head>
  <title>URLockBox</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>
<body>
  <header>
    <% if current_user %>
      <%= link_to 'Sign Out', logout_path, method: :DELETE %>
    <% else %>
      <%= link_to 'Sign In', login_path %>
    <% end %>
  </header>
  <%= render partial: "/layouts/flash" %>
<div class="container">
  <%= yield %>
</div>

</body>
</html>
