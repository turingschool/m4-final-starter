<!DOCTYPE html>
<html>
<head>
  <title>URLockBox</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if flash['warning'] %>
    <div class='alert'>
      <h3><%= flash['warning'] %></h3>
    </div>
  <% end %>
<div class="container">
  <%= yield %>
</div>

</body>
</html>
