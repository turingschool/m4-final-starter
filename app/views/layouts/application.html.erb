<!DOCTYPE html>
<html>
<head>
  <title>URLockBox</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>
<body>
  <% flash.each do |type, message| %>
    <%= content_tag :div, sanitize(message), class: "alert alert-#{type}", class: "flash" %>
  <% end %>
  <div class="container">
    <%= yield %>
  </div>

</body>
</html>
